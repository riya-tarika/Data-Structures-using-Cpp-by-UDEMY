#include<bits/stdc++.h>
using namespace std;

int length(char input[])
{
    int count=0;
    for(int i=0;input[i]!='\0';i++)
    {
        count++;
    }
    return count;
}
int main()
{
    char name[100];
    cin>>name;
    cout<<length(name)<<endl;
    //inbuilt length counter for string or char string strlen(arr)
    cout<<strlen(name);
    return 0;
}