#include<bits/stdc++.h>
using namespace std;
int main(){
    int a[]={1,2,3,4,5};
    int n=sizeof(a)/sizeof(int);
    int s=0,e=n-1;
    while(s<=e)
    {
        swap(a[s],a[e]);
        s++;
        e--;
    }
    for(int i=0;i<n;i++)
    {
        cout<<a[i];
    }
    return 0;
}