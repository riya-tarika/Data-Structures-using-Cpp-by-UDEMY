#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    int a[n];
    int m=sizeof(a)/sizeof(a[0]);
    cout<<"Enter no. of elements: ";
    cin>>n;
    cout<<"\n Enter array elements:"<<endl;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int large=a[0]; //large=INT_MIN
    for(i=0;i<n;i++)
    {
        if(a[i]>large)
        {
            large=a[i];
        }
    }
    cout<<"largest element is: "<<large<<endl;
    return 0;
}